@import '../../../common/tableView.wxss';
.im_tabbar {
    width: 100%;
    height: 84rpx;
    line-height: 84rpx;
    text-align: center;
    background-color: #FFFFFF;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 10;
}
.im_tabbar >view{
    display: inline-block;
}
.im_tabbar_item {
    font-size: 28rpx;
    color: #999999;
}

.im_tabbar_item.active {
    color: #00b955;
}

.right_icon {
    width: 20rpx;
    height: 20rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
}
.wait{
    display: flex;
    width: 480rpx;
    background: #ffffff;
    border: 1rpx solid #f7f7f7;
    border-radius: 35rpx;
    padding: 20rpx;
    margin-left: 32rpx;
    margin-top: 30rpx;
    box-shadow: 0 5rpx 20rpx 0 rgba(48,48,60,0.08);
}
.wait .time_icon{
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}
.wait >view{
    font-size: 28rpx;
    color: #111734;
}
.main_bottom{
    width: 100%;
    height: 70rpx;
}