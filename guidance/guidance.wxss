/* pages/quickConsultation///pages/guidance/guidance.wxss */
@import '../../../common/tableView.wxss';
page {
  background: #F7F7F7
}

.tab-bar {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1;
}

.chat-box {
  background: none;
  padding-top: 84rpx;
  box-sizing: border-box;
  overflow: scroll;
}

.chat-box .cell {
  margin-bottom: 30rpx;
  align-items: flex-start;
}

.chat-box .cell::before {
  border: 0;
}

.chat-box .cell_left {
  width: 80rpx;
  height: 80rpx;
}

.chat-box .cell_left image {
  width: 100%;
  height: 100%;
}

.chat-box .cell_center {
  font-size: 20rpx;
  color: #999999;
  margin: 0 20rpx;
}

.chat-box .cell_center .dialog-box{
  background: #ffffff;
  border-radius: 0px 20rpx 20rpx 20rpx;
  font-size: 30rpx;
  color: #333333;
  padding: 20rpx;
  margin-top: 4rpx;
  max-width: 490rpx;
  box-sizing: border-box;
}

.chat-box .cell_user {
  justify-content: flex-end;
}

.chat-box .cell_user.cell_center {
  text-align: right;
}

.chat-box .cell_user .cell_center .dialog-box {
  background: #00b955;
  color: #FFFFFF;
  border-radius: 20px 0rpx 20rpx 20rpx;
}

.chat-box .cell_user .cell_center .name {
  text-align: right;
}

.inquiry-box {
  margin: 30rpx;
  border-radius: 20rpx;
  background: #FFFFFF;
  overflow: hidden;
}

.inquiry-box .header {
  background: #00b955;
  color: #FFFFFF;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.inquiry-box .header text {
  font-size: 24rpx;
  font-weight: normal;
  margin-left: 10rpx;
}

.inquiry-box .content {
  display: flex;
  align-items: center;
  padding: 30rpx 20rpx;
  font-weight: bold;
  font-size: 30rpx;
}

.inquiry-box .content .left {
  width: 90rpx;
  height: 90rpx;
  border-radius: 90rpx;
  margin-right: 20rpx;
}

.inquiry-box .content .left image {
  width: 100%;
  height: 100%;
}

.inquiry-box .content .red {
  color: #FF2121;
}

.inquiry-box .content .center {
  flex: 1
}

.inquiry-box .content .right {
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  background: #00b955;
  color: #FFFFFF;
  font-weight: bold;
  text-align: center;
}

.inquiry-box .tips {
  font-size: 24rpx;
  color: #41435D;
  padding: 0 20rpx;
}


.coupons-cell {
  display: flex;
  align-items: center;
  padding: 30rpx 20rpx;
  border-top: 1rpx solid #EEEEEE;
  font-size: 28rpx;
  color: #9D9FB8;
  margin-top: 20rpx;
}

.coupons-cell .left {
  flex: 1;
  color: #333333
}

.coupons-cell .left text {
  font-size: 24rpx;
  margin-left: 20rpx;
}

.coupons-cell .right {
  width: 260rpx;
  text-align: right;
}

.coupons-cell .arrow-icon {
  width: 20rpx;
  height: 20rpx;
  margin-left: 20rpx;
}

.coupons-cell .red {
  color: #F93A4A
}

.coupons-cell .blue {
  color: #00b955
}

.bottom-box {
  padding: 30rpx 15rpx 10rpx;
  background: #FFFFFF;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  border-radius: 30rpx 30rpx 0 0;
  box-sizing: border-box;
}

.bottom-box .flex-box {
  display: flex;
  flex-wrap: wrap;
}

.bottom-box .tag {
  padding: 10rpx 40rpx;
  background: #f5f5f5;
  border-radius: 32px;
  margin: 0 15rpx 20rpx ;
  color: #666666;
  font-size: 28rpx;
  display: block;
}

.bottom-box checkbox, .bottom-box radio {
  display: none;
}

.bottom-box textarea {
  background: #F9F9F9;
  height: 160rpx;
  width: 100%;
  border-radius: 16rpx;
  margin: 10rpx 15rpx 30rpx;
  padding: 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  position: relative;
}

.bottom-box .text-count {
  color: #999999;
  font-size: 24rpx;
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
}

.bottom-box .selected {
  color: #00b955;
  background: #e8fff3;
  border: 2rpx solid #00b955;
}

.bottom-box .desc {
  font-size: 24rpx;
  color: #999999;
  margin-bottom: 20rpx;
}

.patient-list-mask .common_section {
  height: 800rpx;
  overflow-y: scroll;
  padding-bottom: 148rpx;
  box-sizing: border-box;
}

.patient-list-mask .cell {
  padding: 0;
  height: 132rpx;
}
.patient-list-mask .cell .idcard_text {
  font-size: 24rpx;
  color: #ff5f1b;
}
.patient-list-mask .cell_center {
  flex: 1
}

.patient-list-mask .cell_right {
  display: flex;
  align-items: center;
}

.patient-list-mask .cell_right .img-box {
  width: 70rpx;
  height: 90rpx;
  text-align: center;
}

.patient-list-mask .cell_right .img-box image {
  width: 36rpx;
  height: 36rpx;
  margin: 27rpx auto;
}

.patient-list-mask .btns{
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 0 30rpx 10rpx;
  width: 100%;
  box-sizing: border-box;
  background: #FFFFFF;
}

.patient-list-mask .btns .tips {
  font-size: 26rpx;
  color: #A9AEB7;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}

.button {
  border-radius: 50rpx;
  height: 88rpx;
  line-height: 88rpx;
  color: #FFFFFF;
  text-align: center;
  font-weight: bold;
  font-size: 36rpx;
  box-sizing: border-box;
  background: #C6CAD4;
  display: block;
  width: 100%;
  margin-bottom: 20rpx;
}

.active{
  background: #00b955;
}

.waiting {
  position: fixed;
  left: 25rpx;
  bottom: 30rpx;
  width: 570rpx;
  height: 70rpx;
  padding: 0 20rpx;
  line-height: 70rpx;
  border-radius: 35rpx;
  box-shadow: 0px 5rpx 20rpx 0px rgba(48,48,60,0.08); 
  display: flex;
  align-items: center;
  background: #FFFFFF;
}

.waiting image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

/* 优惠券弹窗 */
.coupon_alert {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 552rpx;
  /* height: 548rpx; */
  z-index: 1001;
  background: #FF3131;
  color: #FFFFFF;
  border-radius: 12rpx;
  padding-bottom: 20rpx;
}

.coupon_alert .title {
  text-align: center;
  font-size: 34rpx;
  font-weight: bold;
  margin: 10rpx 0;
}

.coupon_alert .desc {
  text-align: center;
  font-size: 24rpx;
  color: #FFD5D5;
}

.coupon_alert  .coupon_section {
  max-height: 410rpx;
  overflow-y: scroll;
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.coupon_alert .coupon_img {
  width: 100%;
  height: 200rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  display: block;
  margin-top: -190rpx;
}

.close_img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  bottom: -120rpx;
  left: 50%;
  margin-left: -30rpx;
  z-index: 1002;
}

.coupon_alert .coupon_section .cell {
  box-sizing: border-box;
  box-shadow: 0;
  border-radius: 8rpx;
  padding: 0;
  height: 156rpx;
  overflow: initial;
}

.coupon_alert .coupon_section .cell:first-child {
  margin-top: 0;
}

.coupon_alert .coupon_section .cell_center {
  width: 70%;
  height: 100%;
  border-right: 2rpx dashed #d6d6d6;
  padding: 20rpx 16rpx;
}

.coupon_alert .coupon_section .cell_center .tag {
  color: #F93A4A;
  border: 1rpx solid #F93A4A;
  font-size: 20rpx;
  background: none;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 30rpx;
  padding: 0 5rpx;
  position: inherit;
  margin-right: 8rpx;
}

.coupon_alert .coupon_section .cell_center .empty {
  margin-top: 25rpx;
  font-size: 20rpx;
  color: #666666;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.coupon_alert .coupon_section .cell_center .text_block {
  margin-bottom: 0;
  font-size: 20rpx;
  color: #999999
}

.coupon_alert .coupon_section .cell_center .name {
  -webkit-line-clamp: 1;
  font-size: 28rpx;
}

.coupon_alert .coupon_section .cell_center .text{
  margin-top: 25rpx;
  font-size: 20rpx;
  color: #666666;
}

.coupon_alert .coupon_section .cell_left {
  background: #fff;
  color: #e4393b;
  width: 180rpx;
  height: 100%;
  position: relative;
}

.coupon_alert .coupon_section .cell_left .dot {
  position: absolute;
  left: 0;
  background: #ff3131;
  width: 16rpx;
  height: 16rpx;
  border-radius: 16rpx;
  margin-left: -8rpx;
}

.coupon_alert .coupon_section .cell_left .dot_top {
  top: -8rpx;
}

.coupon_alert .coupon_section .cell_left .dot_bottom {
  bottom: -8rpx;
}

.coupon_alert .coupon_section .cell_left .price {
  margin: 0;
  font-size: 58rpx;
  height: 156rpx;
  line-height: 156rpx;
}

.coupon_alert .coupon_section .cell_left .price .text {
  font-size: 20rpx;
}

.coupon_alert .coupon_section .cell_left .button {
  border: 1px solid #e02e24;
  border-radius: 15px;
  width: 90rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  margin: 10rpx auto 0;
}

.coupon_alert .coupon_section .cell_left .gray {
  color: #8a8080;
  border-color: #8a8080;
}

.coupons-dialog {
  height: 800rpx;
  overflow-y: scroll;
}
